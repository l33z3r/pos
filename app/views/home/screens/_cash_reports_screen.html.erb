<div id="cash_reports_screen" style="display: none;">

  <div id="receipt_keypad_container">

    <div id="cash_reports_receipt_header">X/Z Total History</div>

    <div id="reports_left_receipt" class="receipt">
      <div id="reports_left_till_roll" class="till_roll"></div>
    </div>

    <div class="clear">&nbsp;</div>

    <div id="cash_reports_keypad" class="keypad">
      <div id="keys" class="keys">
        <% [[7,9],[4,6],[1,3]].each do |x,y| %>
          <% (x..y).each do |num| %>
            <div id="cash_reports_keypad_num_<%= num %>" class="key" onclick="cashReportsScreenKeypadClick(<%= num %>)"><%= num %></div>
          <% end %>
        <% end %>

        <div id="cash_reports_keypad_num_0" class="key" onclick="cashReportsScreenKeypadClick(0)">0</div>
        <div id="cash_reports_decimal_key" class="key" onclick="cashReportsScreenKeypadClick('.')">.</div>
        <div id="cash_reports_cancel_key" class="key" onclick="cashReportsScreenKeypadClickCancel()">C</div>
      </div>

      <div class="clear">&nbsp;</div>

    </div>
  </div>

  <div id="report_coin_counter_container">
    <div id="reports_center_receipt">
      <div id="reports_center_till_roll">
        <div id="cash_total_data_table_container"></div>
      </div>
    </div>

    <div id="coin_counter_container">
      <div id="coin_counter">
        <div id="coin_counter_popout" class="popout_dialog">
          <div id="coin_counter_header" class="header">
            Cash In Drawer Count
          </div>

          <div class="clear_top_margin_10">&nbsp;</div>

          <div id="reports_coin_counter_widget_container"></div>

          <div class="action_buttons">
            <div class="button" onclick="cancelCashReportCoinCount()">
              <div class="icon">
                <%= image_tag "button_logos/cancel_button.png" %>
              </div>
              <div class="button_name">Cancel</div>
            </div>

            <div id="tendered_button" class="button" onclick="saveCashReportCoinCount()">
              <div class="icon">
                <%= image_tag "button_logos/ok_button.png" %>
              </div>
              <div class="button_name">Ok</div>
            </div>
          </div>

          <div class="clear">&nbsp;</div>

          <div id="finish_cash_total_action_button" class="action_buttons">
            <div class="button" onclick="finishCashTotal()">
              <div class="icon">
                <%= image_tag "button_logos/finish_cash_total.png" %>
              </div>
              <div class="button_name">Finish</div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="clear">&nbsp;</div>
  </div>
</div>