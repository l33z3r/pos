<div id="edit_price_screen">
  <div id="header">

    <div id="back_button" class="small_button" onclick="backScreenNav();">Back</div>

    <div class="title">Change Price</div>

    <div id="options_button" class="small_button" onclick="setNewPrice($('.new_price').val());">Save</div>

    <div class="clear">&nbsp;</div>
  </div>

  <div id="keypad_container">

    <div class="clear">&nbsp;</div>

    <div id="entered_table_number_container">
      <div id="price_number_show"></div>
    </div>

    <div id="keypad" class="keypad">
      <div id="keys" class="price_keys">
        <% [[7,9],[4,6],[1,3]].each do |x,y| %>
          <% (x..y).each do |num| %>
            <div id="num_<%= num %>" class="key" onclick="priceNumberSelectKeypadClick(<%=num%>)"><%= num %></div>
          <% end %>
        <% end %>

        <div id="num_0" class="key" onclick="priceNumberSelectKeypadClick('0')">0</div>
        <div id="num_00" class="key" onclick="priceNumberSelectKeypadClick('00')">00</div>
        <div class="cancel_key" class="key" onclick="doCancelpriceNumberSelectKeypad()">C</div>
      </div>
    </div>
  </div>
</div>

